<div class="blog-list">
  <% blogs.forEach(({_id, title, content, userId}) => { %>
  <a href="blogs/<%= _id %>">
    <div class="blog">
      <img
        class="blog-image"
        src="https://dl.airtable.com/.attachments/a0cd0702c443f31526267f38ea5314a1/2447eb7a/paris.jpg"
        alt="<%= title %> "
      />
      <div class="blog-footer">
        <h2 class="blog-title"><%= title %></h2>
        <p class="blog-content"><%= content.substring(0, 100) + " ... " %></p>
        <span class="blog-author">By: <%= userId.name %></span>
      </div>
    </div>
  </a>
  <% })%>
</div>
